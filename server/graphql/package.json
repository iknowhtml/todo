{
  "name": "todo-graphql",
  "description": "GraphQL server for my Todo application",
  "version": "0.0.2",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon --exec babel-node src",
    "build": "webpack --mode production --progress --display verbose --display-error-details --colors",
    "package": "docker build -t iknowhtml/todo-graphql:$CURRENT_TAG iknowhtml/todo-graphql:latest .",
    "push": "docker push iknowhtml/todo-graphql",
    "deploy": "echo \"deploying $CURRENT_TAG...\"",
    "test": "jest",
    "lint": "eslint --fix src/ tests/",
    "clean": "rm -rf dist/*"
  },
  "repository": "https://github.com/iknowhtml/node-template.git",
  "author": "Aki Gao",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.9.1"
  }
}
